{
  "name": "the-left-phalange",
  "version": "1.0.0",
  "scripts": {
    "build-game-css": "node-sass --include-path app app/components/game/**.scss app/components/game/game.component.css",
    "watch-all": "tsc && concurrently \"npm run watch-game-css\" \"npm run tsc:w\"",
    "watch-game-css": "nodemon -e scss -x \"npm run build-game-css\"",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "live-server --open=app/domain.tests/domain.tests.html"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.2",
    "@angular/compiler": "2.0.0-rc.2",
    "@angular/core": "2.0.0-rc.2",
    "@angular/http": "2.0.0-rc.2",
    "@angular/platform-browser": "2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "2.0.0-rc.2",
    "@angular/router": "2.0.0-rc.2",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.2",
    "angular2-in-memory-web-api": "0.0.12",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "gulp-concat": "^2.6.0",
    "gulp-typescript": "^2.13.6",
    "live-server": "^1.0.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "gulp-sass": "^2.3.2",
    "jasmine-core": "2.4.1",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
